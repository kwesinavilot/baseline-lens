{
    "baseline-lens.showInlineIndicators": false,
    "baseline-lens.supportThreshold": 90,
    "baseline-lens.excludePatterns": [
        "**/node_modules/**",
        "**/dist/**",
        "**/build/**"
    ],
    "baseline-lens.baselineStatusMapping": {
        "widely_available": "info",
        "newly_available": "warning",
        "limited_availability": "error"
    },
    "baseline-lens.enabledAnalyzers": {
        "css": true,
        "javascript": true,
        "html": true
    },
    "baseline-lens.maxFileSize": 10485760,
    "baseline-lens.analysisTimeout": 5000,
    "baseline-lens.enabledFileTypes": [
        "css",
        "scss",
        "less",
        "javascript",
        "typescript",
        "html",
        "vue",
        "svelte"
    ],
    "baseline-lens.diagnosticSeverity": "warning",
    "baseline-lens.customBrowserMatrix": [],
    "baseline-lens.enableTeamConfig": true,
    "baseline-lens.showEducationalHints": true,
    "baseline-lens.autoRefreshOnSave": true,
    "baseline-lens.showDiagnostics": true
}