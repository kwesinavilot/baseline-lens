{
  "supportThreshold": 90,
  "customBrowserMatrix": [],
  "excludePatterns": [
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/coverage/**",
    "**/.git/**",
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**",
    "**/coverage/**",
    "**/.git/**"
  ],
  "includePatterns": [
    "**/*.css",
    "**/*.scss",
    "**/*.sass",
    "**/*.less",
    "**/*.js",
    "**/*.jsx",
    "**/*.ts",
    "**/*.tsx",
    "**/*.mjs",
    "**/*.html",
    "**/*.htm",
    "**/*.vue",
    "**/*.svelte"
  ],
  "enabledAnalyzers": {
    "css": true,
    "javascript": true,
    "html": true
  },
  "maxFileSize": 10485760,
  "analysisTimeout": 5000
}