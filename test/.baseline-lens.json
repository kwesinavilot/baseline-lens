{
  "supportThreshold": 90,
  "excludePatterns": [
    "**/node_modules/**",
    "**/dist/**",
    "**/build/**"
  ],
  "baselineStatusMapping": {
    "widely_available": "info",
    "newly_available": "warning",
    "limited_availability": "error"
  },
  "enabledAnalyzers": {
    "css": true,
    "javascript": true,
    "html": true
  },
  "maxFileSize": 10485760,
  "analysisTimeout": 5000
}